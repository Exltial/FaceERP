<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ncut.face.erp.service.user.repository.UserRepository">
    <select id="getAllTenantId" resultType="java.lang.String">
        SELECT DISTINCT tenant_id
        FROM erp_user
    </select>
    <insert id="insertUser" parameterType="com.ncut.face.erp.service.login.domain.UserRegistryModel">
        INSERT INTO erp_user (tenant_id, user_name, password, user_role, face_id, create_time)
        VALUES (#{tenantId}, #{userName}, #{password}, #{userRole}, #{picId}, now())
    </insert>
</mapper>